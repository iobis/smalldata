{
  "name": "@smalldata/occurrences-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "run-s css:build js:build",
    "css:build": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "css:watch": "npm run css:build && node-sass-chokidar --include-path ./src --include-path ../node_modules src/ -o src/ --watch --recursive",
    "eject": "react-app-rewired eject",
    "js:build": "react-app-rewired build",
    "js:start": "react-app-rewired start",
    "start": "run-p css:watch js:start",
    "test": "react-app-rewired test --env=jsdom",
    "test:ci": "cross-env CI=true react-app-rewired test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-solid-svg-icons": "5.7.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@smalldata/dwca-lib": "1.0.0",
    "bulma": "0.7.4",
    "classnames": "2.2.6",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "jest": "23.6.0",
    "jest-dom": "3.0.0",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "4.1.5",
    "react-app-rewire-yarn-workspaces": "1.0.3",
    "react-app-rewired": "2.0.3",
    "react-test-renderer": "16.8.1"
  }
}
